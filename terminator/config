      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
  [[EditorPlugin]]
    command = {filepath}
    editor = code
    match = [^ \t\n\r\f\v:]+?\.(ini|conf|me|txt|xml|json|py|go) *[\n:]?([0-9]+)*
